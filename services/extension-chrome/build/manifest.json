{
    "manifest_version": 2,
    "name": "WhatsPlus",
    "version": "0.1",
    "content_scripts": [
        {
          "matches": ["https://web.whatsapp.com/*"],
          "js": ["content.js"]
        }
    ],
    "background" : {
        "scripts" : ["background.js"]
    },
    "browser_action": {},
    "permissions": ["activeTab"]
}